{
  "name": "ksface",
  "version": "2.0.0",
  "description": "a real minimalistic frontend boilerplate",
  "main": "src/index.js",
  "scripts": {
    "build": "sass src/index.scss dist/ksface.css --style compressed && webpack --config webpack.config.js",
    "watch": "sass --watch src/index.scss dist/ksface.css && webpack --config webpack.config.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kevinschmidt777/ksface"
  },
  "author": "Kevin Schmidt",
  "license": "MIT",
  "prettier": {
    "singleQuote": true,
    "arrowParens": "always",
    "proseWrap": "never",
    "printWidth": 140
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "babel-loader": "8.2.2",
    "prettier": "2.2.1",
    "sass": "1.32.6",
    "webpack": "5.19.0",
    "webpack-cli": "4.4.0"
  }
}
